<% content_for :content_header  do %>
  <section class="content-header">
    <h1>Stocks <small>#new</small></h1>
  </section>
<% end %>

<section class="content invoice">
   <%= form_for(Stock.new) do |s| %>
              <div class="form_group">
                <%= s.label "Ingrese stock del producto" %>
                <%= s.text_field :inicial_stock %>
              </div>
              <%= s.submit "Guardar" %>
    <%end%>
</section>